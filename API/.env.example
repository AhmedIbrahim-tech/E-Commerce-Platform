# Database Configuration
ConnectionStrings__DefaultConnection=Server=localhost;Database=ECommerceDB;Trusted_Connection=True;TrustServerCertificate=True;

# JWT Authentication Settings
jwtSettings__Secret=your-super-secret-jwt-key-change-in-production
jwtSettings__Issuer=MyApp
jwtSettings__Audience=MyAppUsers
jwtSettings__AccessTokenExpireDate=7
jwtSettings__RefreshTokenExpireDate=30
jwtSettings__ValidateIssuer=true
jwtSettings__ValidateIssuerSigningKey=true
jwtSettings__ValidateAudience=true
jwtSettings__ValidateLifeTime=true

# Email Settings (SMTP)
emailSettings__FromEmail=your-email@example.com
emailSettings__Host=smtp.gmail.com
emailSettings__Port=587
emailSettings__UserName=your-smtp-username
emailSettings__Password=your-smtp-password
emailSettings__UseSSL=false

# Encryption Settings
encryptionSettings__Key=your-strong-encryption-key-32-characters-min

# Identity Tables Configuration
IdentityTables__Users=Users
IdentityTables__Roles=Roles
IdentityTables__UserClaims=UserClaims
IdentityTables__UserRoles=UserRoles
IdentityTables__UserLogins=UserLogins
IdentityTables__RoleClaims=RoleClaims
IdentityTables__UserTokens=UserTokens

# Database Tables Configuration
DatabaseTables__LogsTable=Logs
DatabaseTables__CacheTable=Cache
DatabaseTables__CacheSchema=dbo

# Stripe Payment Settings
StripeSettings__PublishableKey=pk_test_your_stripe_publishable_key
StripeSettings__SecretKey=sk_test_your_stripe_secret_key

# Paymob Payment Gateway Settings
Paymob__ApiKey=your_paymob_api_key
Paymob__SecretKey=your_paymob_secret_key
Paymob__PublicKey=your_paymob_public_key
Paymob__HMAC=your_paymob_hmac_key
Paymob__CardIntegrationId=your_card_integration_id
Paymob__MobileIntegrationId=your_mobile_integration_id
Paymob__IframeId=your_iframe_id

# Google OAuth Settings
Authorization__Google__ClientId=your-google-client-id.apps.googleusercontent.com
Authorization__Google__ClientSecret=your-google-client-secret

# ASP.NET Core Environment
ASPNETCORE_ENVIRONMENT=Development
